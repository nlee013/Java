package com.score6;

import java.io.Serializable;

class MyDataVO implements Serializable{
	
	private String name;
	private int data;
	
	public MyDataVO(String name, int data) {
		
		this.name = name;
		this.data = data;
		
	}
	
	@Override
	public String toString() {
		return name + ":" + data;
	}
}

public class Day17Assign {

	public static void main(String[] args) {

	}

}
