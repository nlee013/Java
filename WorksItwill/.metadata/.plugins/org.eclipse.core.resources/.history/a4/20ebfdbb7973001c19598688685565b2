package com.score6;

import java.io.Serializable;

public class DataVO implements Serializable{
		
		private String name, birth;
		private int data, score;
		
		public DataVO(String name, String birth, int data, int score) {
			
			this.name = name;
			this.birth = birth;
			
			this.data = data;
			this.score = score;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

		public int getData() {
			return data;
		}

		public void setData(int data) {
			this.data = data;
		}

		public String getBirth() {
			return birth;
		}

		public void setBirth(String birth) {
			this.birth = birth;
		}

		public int getScore() {
			return score;
		}

		public void setScore(int score) {
			this.score = score;
		}
		
}
