package com.score4;
//매우 중요
//Value Object : Data를 저장
//DTO : Data Transform Object

public class ScoreVO {
	
	private String hak,name;
	private int kor, eng, mat;

	public ScoreVO(){//기본 생성자
		
	}
	
	public ScoreVO(String hak, String name, int kor, int eng, int mat) {
		
		this.hak = hak;
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
		
	}
	
	public void setData(String hak, String name, int kor, int eng, int mat) {
		
		this.hak = hak;
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
	}
	
	
}
