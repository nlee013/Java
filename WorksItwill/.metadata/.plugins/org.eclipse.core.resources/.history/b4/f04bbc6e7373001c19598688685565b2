package com.day17;

import java.io.Serializable;

public class MyDataVO implements Serializable{

	private static final long serialVersionUID = -2678074349520400586L;
	private String name;
	private int score;
	
	//생성자를 오버로딩하기
	public MyDataVO(String name, int score) {
		
		this.name = name;
		this.score = score;
	}

	//원래는 항상 해야되지만 이번에는 위에 생성자를 만들어서 써보자!
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}
	
	
}
