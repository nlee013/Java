	public void fish1() throws Exception {
		
		try {
             
             Thread.sleep(0);
        }catch(InterruptedException e) {
             e.printStackTrace();
        }
		
		String[] fishT = {
		
				" o 								" , 
				"o      ______/~/~/~/__           /(( " , 
				"  o  // __            ====__    /_(( " , 
				" o  //  @))       ))))      ===/__(( " , 
				"    ))           )))))))        __(( " , 
				"    \\\\     \\)     ))))    __===\\ _(( " , 
				"     \\\\_______________====      \\_(( " , 
				"                                 \\((",
		 ""};
		
		for (int i = 0; i < fishT.length; i++) {
		    Thread.sleep(450);
			System.out.println(fishT[i]);
		}	
	}    
	
