package com.day16;

class MyThread2 implements Runnable{
	
	private int num;
	private String name;
	
	public MyThread2(int num, String name) {
		
		this.num = num;
		this.name = name;
	}
	
	@Override
	public void run() {
		
		int i = 0;
		
		while(i < num) {
			
			//여기엔 getName()이 없음
			System.out.println(name + ":" + i);
			 i++;
			 
			 try {
				Thread.sleep(100);
			} catch (Exception e) {
				
			}
		}
	}
}
public class Test2day16 {

	public static void main(String[] args) {

	}

}
