package com.day9;


class TestC{
	
	private String title;
	protected int area;
	
	public TestC(String title) {
		this.title = title;
	}
	public void print() {
		System.out.println("title: " + area);
	}
}

class RectC extends TestC{
	private int w, h;
	
	public RectC(int w, int h) {
		this.w = w;
		this.h = h;
	}
	
	public void rectArea() {
		area = w*h;
	}
}

public class Test46 {

	public static void main(String[] args) {
		
	}

}
