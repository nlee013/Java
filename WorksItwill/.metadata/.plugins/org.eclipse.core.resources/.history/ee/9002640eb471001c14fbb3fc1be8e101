package com.day15;

//Generic -> list<000>

class Box<T>{
	
	private T t;
	
	public void set(T t) {
		
		this.t = t;
	}
	
	public T get() {
		return t;
	}
	
	
}
public class Test1day15 {

	public static void main(String[] args) {

		//Box<>안에 들어간 모든게 integer가 됨 
		Box <Integer> b1 = new Box<Integer>();
		
		b1.set(10);
		int i = b1.get();
		System.out.println(i);
		
		
	}

}
