package com.day12;

class SuperTest{
	
	public int a = 10, b = 20;
	
	public void write() {
		System.out.println("Super class write() method...");
	}
	
	public int hap() {
		return a + b; //30
	}
}

class subTest extends SuperTest{
	
	public int b =100, c = 200;
	
	public void print() {
		System.out.println("Sub class print() method...");
	}
}

