package com.score2;

public class Ranking {

	String name;

	int[] score2 = new int[3];
	
	private int rank;
	
	int tot;
	
	Score2 scr;
	
		public void set(int rank) {
			this.rank = rank;
		}
		
		public void ranking() {
			
			scr = new Score2();
			
			for(int i = 0; i < scr.inwon; i++) {
				
				rank = 1;
			}
			
			for(int i = 0; i < scr.inwon; i++) {
				
				for(int j = 0; j < scr.inwon; j++) {
					
					if(scr.rk[i].tot > scr.rk[j].tot) {
						scr.rk[j].rank++;
					}else if() {
						
					}
				}
				
			}
		}
//		public void print() {
//			System.out.println(name + ":" + score + rank);
//		}
}
